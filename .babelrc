{
  presets: [
    "@babel/preset-react",
    [
      "@babel/preset-env",
      {
        targets: {
          browsers: ["last 2 versions"]
        }
      }
    ],
    "@emotion/babel-preset-css-prop"
  ],
  plugins: [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-syntax-dynamic-import",
    "transform-inline-environment-variables"
  ],
  env: {
    test: {
      plugins: ["dynamic-import-node"]
    }
  }
}
